<%= form_for @dog do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <%= f.label :breed %>
  <%= f.text_field :breed %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :bio %>
  <%= f.text_field :bio %>
  <%= f.label :personality %>
  <%= f.text_field :personality %>
  <%= f.label :play %>
  <%= f.text_field :play %>
  <%= f.label :size %>
  <%= f.text_field :size %>

  <%= f.submit 'Update', id: "update-dog" %>
<% end %>
